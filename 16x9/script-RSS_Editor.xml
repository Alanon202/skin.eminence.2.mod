<!--based on PMIII DialogMediaSource.xml by Jezz_X & chokemaniac-->
<window>
    <defaultcontrol always="true">10</defaultcontrol>
    <coordinates>
        <origin x="0" y="0" />
    </coordinates>
    <controls>
        <include>GlobalOverlay</include>
        <control type="group">
            <include>Animation_SlideIn</include>
            <include>Animation_FadeOut</include>
            <width>1000</width>
            <height>500</height>
            <centerleft>50%</centerleft>
            <centertop>50%</centertop>
        <control type="group">
            <description>background image</description>
            <width>1000</width>
            <height>500</height>
            <include>Dialog_Background</include>
        </control>
        <control type="label" id="2">
            <description>Heading Label</description>
            <posy>-400</posy>
            <width>1000</width>
            <align>center</align>
            <textcolor>ListLabel</textcolor>
        </control>
        <control type="label" id="4">
            <top>-290</top>
            <width>760</width>
            <left>40</left>
            <font>Font-ListInfo-Small</font>
            <description>List Label</description>
        </control>
        <control type="list" id="10">
            <left>40</left>
            <top>140</top>
            <width>690</width>
            <height>240</height>
            <onup>9001</onup>
            <onleft>13</onleft>
            <onright>11</onright>
            <ondown>9001</ondown>
            <pagecontrol></pagecontrol>
            <scrolltime>200</scrolltime>
            <itemlayout height="80">
                <control type="image">
                    <width>690</width>
                    <height>69</height>
                    <texture colordiffuse="PosterBorder">common/rsseditor-nofocus.png</texture>
                </control>
                <control type="label">
                    <posx>5</posx>
                    <posy>0</posy>
                    <width>690</width>
                    <height>69</height>
                    <aligny>center</aligny>
                    <align>left</align>
                    <font>Font-ListInfo-Small</font>
                    <info>ListItem.Label</info>
                </control>
            </itemlayout>
            <focusedlayout height="80">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>690</width>
                    <height>69</height>
                    <texture colordiffuse="PosterBorder">common/rsseditor-nofocus.png</texture>
                    <visible>!Control.hasfocus(10)</visible>
                </control>
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>690</width>
                    <height>69</height>
                    <texture colordiffuse="PosterBorder">common/rsseditor-focus.png</texture>
                    <visible>Control.hasfocus(14)|Control.hasfocus(10)</visible>
                </control>
                <control type="label">
                    <posx>5</posx>
                    <posy>0</posy>
                    <width>690</width>
                    <height>69</height>
                    <aligny>center</aligny>
                    <align>left</align>
					<scroll>yes</scroll>
                    <font>Font-ListInfo-Small</font>
                    <info>ListItem.Label</info>
                </control>
            </focusedlayout>
        </control>
        <control type="group" id="9000">            
            <control type="button" id="11">
                <description>Change Set Button</description>
                <right>40</right>
                <top>140</top>
                <align>center</align>
                <width>200</width>
                <height>69</height>
                <onup>14</onup>
                <onleft>10</onleft>
                <onright>10</onright>
                <ondown>13</ondown>
                <include>Defs_OptionButton2</include>
            </control>
            <control type="button" id="13">
                <description>Add Button</description>
                <right>40</right>
                <top>220</top>
                <align>center</align>
                <width>200</width>
                <height>69</height>
                <label>15019</label>
                <onup>11</onup>
                <onleft>10</onleft>
                <onright>10</onright>
                <ondown>14</ondown>
                <include>Defs_OptionButton2</include>
            </control>
            <control type="button" id="14">
                <description>Remove Button</description>
                <right>40</right>
                <top>300</top>
                <align>center</align>
                <width>200</width>
                <height>69</height>
                <label>1210</label>
                <onup>13</onup>
                <onleft>10</onleft>
                <onright>10</onright>
                <ondown>9001</ondown>
                <include>Defs_OptionButton2</include>
            </control>
        </control>
        <control type="group" id="9001">
            <control type="button" id="18">
                <description>Ok Button</description>
                <align>center</align>
                <right>250</right>
                <top>400</top>
                <width>200</width>
                <height>69</height>
                <label>186</label>
                <onup>10</onup>
                <onleft>10</onleft>
                <onright>19</onright>
                <ondown>10</ondown>
                <include>Defs_OptionButton2</include>
            </control>
            <control type="button" id="19">
                <description>Cancel Button</description>
                <align>center</align>
                <right>40</right>
                <top>400</top>
                <width>200</width>
                <height>69</height>
                <label>222</label>
                <onup>10</onup>
                <onleft>18</onleft>
                <onright>11</onright>
                <ondown>10</ondown>
                <include>Defs_OptionButton2</include>
            </control>
        </control>
       </control> 
    </controls>
</window>