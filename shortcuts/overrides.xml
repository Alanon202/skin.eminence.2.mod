	<!-- Parental control -->
	<override action="globaloverride">
		<condition>String.Contains(Container(9000).ListItem.Property(ParentalControl),yes) + !String.IsEqual(Skin.String(mypassword),Skin.String(passcode))</condition>
		<action>Skin.SetString(dialog_action,::ACTION::)</action>
		<action>ActivateWindow(1132)</action> 
		<action>AlarmClock(parental,Skin.Reset(mypassword),$VAR[password_time],Silent)</action>	
	</override>
	<override action="globaloverride">
		<condition>[String.IsEmpty(Container(9000).ListItem.Property(ParentalControl)) | String.Contains(Container(9000).ListItem.Property(ParentalControl),no)] | String.IsEqual(Skin.String(mypassword),Skin.String(passcode))</condition>
		<action>AlarmClock(parental,Skin.Reset(mypassword),$VAR[password_time],Silent)</action>
		<action>::ACTION::</action>
	</override>

